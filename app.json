{
  "expo": {
    "name": "Canteen Admin",
    "slug": "CanteenAdmin",
    "platforms": ["ios", "android"],
    "android": {
      "package": "com.tushar.AdminCOA",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "An app for canteen owners to manage orders from Canteen Ordering."
  }
}
